import numpy as np


# Initialize a 3 by 2 matrix
A = np.array([[1, 2], [4, 5], [8, 9]])


# Initialize a 2 by 1 matrix
B = np.array([[1], [4]])

# We expect a resulting matrix of (3 by 2)*(2 by 1) = (3 by 1)
mult_AB = A @ B

# Make sure you understand why we got that result
print(A)
print(B)

print(mult_AB)
